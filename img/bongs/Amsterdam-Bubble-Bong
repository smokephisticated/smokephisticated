                KlaviyoSubscribe.attachToForms('#email_signup');
                form.on('klaviyo.subscribe.error', function(){
                    form.find('.input-box').addClass('validation-error')
                        .find('input').addClass('validation-failed').one('focus', function(){
                        form.find('.input-box').removeClass('validation-error')
                            .find('input').removeClass('validation-failed').val('').end()
                            .find('.validation-advice').hide();
                    })
                }).on('klaviyo.subscribe.success', function(){
                    form.parent().find('.messages').show().find('.placeholder').html(form.find('.success_message').html());
                    setTimeout(function(){  form.parent().find('.messages').hide() },5000)
                })
            })(jQuery);
        });
    </script>
</div></div> <div class="footer-container">

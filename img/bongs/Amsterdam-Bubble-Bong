<script type="text/javascript"> 
function searchsubmit(form){ 
var w = encodeURIComponent(form.w.value); 
var curr = encodeURIComponent(form.curr.value); 
var store = encodeURIComponent(form.store.value); 
var view = encodeURIComponent(form.view.value); 
window.location="//www.grasscity.com/search/go?w="+w+"&curr="+curr+"&store="+store+"&view="+view; 
return false; 
} 
